model Pedidos {
  id                   Int    @id @default(autoincrement())
  id_cliente           String
  estado               Int
  id_platos            String
  monto_total          Float
  porcentaje_descuento Int
  domicilio_entrega    String

  cliente   Cliente           @relation(fields: [id_cliente], references: [id])
  domicilio Domicilio_Cliente @relation(fields: [domicilio_entrega], references: [id])

  @@map("pedidos")
}
